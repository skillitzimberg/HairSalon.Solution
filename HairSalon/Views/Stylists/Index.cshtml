@{
  Layout = "_Layout";
}
<div class="container index">
  <nav class="navbar navbar-inverse navbar-fixed-top">
    <span class="logo"><a href="/">HairSalon</a></span>
    <ul>
      <li><a href="#">About</a></li>
    </ul>
  </nav>
  <h1 id="banner">Stylists</h1>
  @if (@Model.Count == 0)
  {
    <h3>No stylists have been added yet!</h3>
  }

  @foreach (Stylist stylist in Model)
  {
    <h3><a href='/stylists/@stylist.GetId()'>@stylist.GetFirstName() @stylist.GetLastName()</a></h3>
  }

  <p><a href='/'>Back home</a></p>
</div>
